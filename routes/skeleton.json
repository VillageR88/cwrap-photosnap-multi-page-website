{
  "head": {
    "link": [
      {
        "rel": "shortcut icon",
        "href": "./favicon.ico",
        "type": "image/x-icon"
      },
      {
        "rel": "prefetch",
        "href": "./stories"
      },
      {
        "rel": "prefetch",
        "href": "./features"
      },
      {
        "rel": "prefetch",
        "href": "./pricing"
      }
    ]
  },
  "element": "cwrap-template",
  "text": "cwrapTemplate[body]",
  "omit": "footerBeta",
  "passover": [
    {
      "element": "ul",
      "extend": [
        {
          "extension": " > li:nth-of-type(even) > div",
          "style": "flex-direction: row-reverse;"
        }
      ],
      "style": "box-sizing: border-box; display: flex; flex-direction: column; list-style: none; margin: 0; padding: 0; width: 100%;",
      "blueprint": {
        "count": "3",
        "element": "li",
        "style": "background-color: white; display: flex;",
        "children": [
          {
            "element": "div",
            "mediaQueries": [
              {
                "query": "max-width: 840px",
                "style": "flex-direction: column-reverse; max-width: unset;"
              }
            ],
            "style": "display: flex; margin-inline: auto; max-width: 1440px; width: 100%;",
            "children": [
              {
                "element": "div",
                "mediaQueries": [
                  {
                    "query": "max-width: 840px",
                    "style": "box-sizing: border-box; max-width: unset; min-height: unset; min-width: unset; padding-block: 72px; padding-inline: 24px;"
                  }
                ],
                "style": "align-items: center; background-color: cwrapArray[black,white,white]; display: flex; max-width: 610px; min-height: 650px; min-width: 495px; position: relative; width: 100%;",
                "children": [
                  {
                    "element": "div",
                    "style": "color: cwrapArray[white,black,black]; display: flex; flex-direction: column; gap: 48px; margin-inline: auto; max-width: 387px; width: 100%;",
                    "children": [
                      {
                        "element": "section",
                        "style": "display: flex; flex-direction: column; gap: 21px;",
                        "children": [
                          {
                            "element": "cwrapArray[div,cwrapOmit,cwrapOmit]",
                            "mediaQueries": [
                              {
                                "query": "max-width: 840px",
                                "style": "height: 6px; left: unset; top: 0; width: 128px;"
                              }
                            ],
                            "style": "background: linear-gradient(065deg, #FFC593 0%, #BC7198 32%, #5A77FF 100%); height: 304px; left: 0; position: absolute; width: 6px;"
                          },
                          {
                            "element": "cwrapArray[h1,h2,h2]",
                            "attributes": {
                              "class": "title"
                            },
                            "text": "cwrapArray[CREATE AND SHARE YOUR PHOTO STORIES.,BEAUTIFUL STORIES EVERY TIME,DESIGNED FOR EVERYONE]"
                          },
                          {
                            "element": "p",
                            "style": "opacity: 0.6;",
                            "attributes": {
                              "class": "description"
                            },
                            "text": "cwrapGlobal[cwrapArray[homeDesc1,homeDesc2,homeDesc3]]"
                          }
                        ]
                      },
                      {
                        "element": "cwrap-template",
                        "text": "cwrapTemplate[arrowLink(arrowColor=cwrapArray[white,black,black],arrowLinkText=cwrapArray[GET AN INVITE,VIEW THE STORIES,VIEW THE STORIES],underlineAnimation=underline-animation)]"
                      }
                    ]
                  }
                ]
              },
              {
                "element": "div",
                "children": [
                  {
                    "element": "picture",
                    "children": [
                      {
                        "element": "source",
                        "attributes": {
                          "media": "(max-width: 480px)",
                          "srcset": "static/images/home/mobile/cwrapArray[create-and-share,beautiful-stories,designed-for-everyone].jpg"
                        }
                      },
                      {
                        "element": "img",
                        "style": "display: flex; height: 100%; object-fit: cover; object-position: cwrapArray[center,20% 0,center]; width: 100%;",
                        "attributes": {
                          "alt": "cwrapArray[Create and Share,Beautiful Stories,Designed for Everyone]",
                          "src": "static/images/home/desktop/cwrapArray[create-and-share,beautiful-stories,designed-for-everyone].jpg"
                        }
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    {
      "element": "cwrap-template",
      "text": "cwrapTemplate[gallery(galleryDate=cwrapOmit)]"
    },
    {
      "element": "cwrap-template",
      "text": "cwrapTemplate[ulFeatures(ulFeaturesCounter=3))]"
    }
  ]
}