{
  "element": "cwrap-template",
  "text": "cwrapTemplate[body]",
  "passover": [
    {
      "element": "main",
      "style": "max-width: 1440px; width: 100%; display: flex; flex-direction: column;",
      "children": [
        {
          "element": "ul",
          "extend": [
            {
              "extension": " > li:nth-of-type(even)",
              "style": "flex-direction: row-reverse;"
            }
          ],
          "style": "display: flex; flex-direction: column; width: 100%; margin: 0; padding: 0; line-style: none;",
          "blueprint": {
            "count": "3",
            "element": "li",
            "style": "display: flex; ",
            "children": [
              {
                "element": "div",
                "style": "background-color: cwrapArray[black,white,white]; max-width: 610px; width: 100%; display:flex; align-items: center; justify-content: center;",
                "children": [
                  {
                    "element": "div",
                    "style": "gap: 48px; max-width: 387px; width: 100%; display: flex; flex-direction: column; color: cwrapArray[white,black,black];",
                    "children": [
                      {
                        "element": "section",
                        "style": "display: flex; flex-direction: column; gap: 21px;",
                        "children": [
                          {
                            "element": "cwrapArray[h1,h2,h2]",
                            "attributes": {
                              "class": "title"
                            },
                            "text": "cwrapArray[CREATE AND SHARE YOUR PHOTO STORIES.,BEAUTIFUL STORIES EVERY TIME,DESIGNED FOR EVERYONE]"
                          },
                          {
                            "element": "p",
                            "style": "opacity: 0.6;",
                            "attributes": {
                              "class": "description"
                            },
                            "text": "cwrapGlobal[cwrapArray[homeDesc1,homeDesc2,homeDesc3]]"
                          }
                        ]
                      },
                      {
                        "element": "cwrap-template",
                        "text": "cwrapTemplate[arrowLink(arrowColor=cwrapArray[white,black,black],arrowLinkText=cwrapArray[GET AN INVITE,VIEW THE STORIES,VIEW THE STORIES],underlineAnimation=underline-animation)]"
                      }
                    ]
                  }
                ]
              },
              {
                "element": "img",
                "style": "width: 100%; display: flex;",
                "attributes": {
                  "src": "static/images/home/desktop/cwrapArray[create-and-share,beautiful-stories,designed-for-everyone].jpg"
                }
              }
            ]
          }
        },
        {
          "element": "ul",
          "style": "display: grid; grid-template-columns: repeat(4, 1fr); list-style: none; margin: 0; padding: 0; box-sizing: border-box;",
          "blueprint": {
            "count": "4",
            "element": "li",
            "children": [
              {
                "element": "a",
                "attributes": {
                  "href": "#"
                },
                "style": "transition: 0.3s; z-index:1; text-decoration: none; min-height: 500px; display: flex; align-items: end; position: relative; cursor: pointer;",
                "extend": [
                  {
                    "extension": ":hover",
                    "style": "transform: translateY(-24px) !important;"
                  },
                  {
                    "extension": "::before",
                    "style": "content: \"\"; position: absolute; left: 50%; height: 6px; background: linear-gradient(0.6deg, #FFC593 0%, #BC7198 52%, #5A77FF 100%); width: 0%; bottom: -5px; z-index: -1; transition: width 0.3s ease-in-out, left 0.3s ease-in-out;"
                  },
                  {
                    "extension": ":hover::before",
                    "style": "width: 100%; left: 0; transform: translateX(0) scale(1,1.05); filter: drop-shadow(0 0 6px cwrapProperty[arrowColor=arrowColor]);"
                  }
                ],
                "children": [
                  {
                    "element": "div",
                    "style": "background: linear-gradient(to bottom, transparent 0%, rgba(0, 0, 0, 0.66) 100%); width: 100%; height: 350px; display: flex;",
                    "children": [
                      {
                        "element": "img",
                        "style": "position: absolute; z-index: -1; top: 0;",
                        "attributes": {
                          "src": "static/images/stories/desktop/cwrapArray[mountains,cityscapes,18-days-voyage,architecturals].jpg"
                        }
                      },
                      {
                        "element": "div",
                        "style": "max-width: 280px; width: 100%; min-height: 99px; margin: auto auto 40px auto; color: white;",
                        "children": [
                          {
                            "element": "section",
                            "style": "display: flex; flex-direction: column; gap: 8px;",
                            "children": [
                              {
                                "element": "h2",
                                "attributes": {
                                  "class": "title-gallery"
                                },
                                "text": "cwrapGlobal[gallery.cwrapIndex.title]"
                              },
                              {
                                "element": "p",
                                "attributes": {
                                  "class": "author-gallery"
                                },
                                "text": "cwrapGlobal[gallery.cwrapIndex.author]"
                              }
                            ]
                          },
                          {
                            "element": "div",
                            "style": "border-bottom: 1px solid #FFFFFF; opacity: 0.25; width: 100%; margin-block: 16px 20px;"
                          },
                          {
                            "element": "cwrap-template",
                            "text": "cwrapTemplate[arrowLinkImitation(arrowEvents=none,arrowColor=white,arrowLinkText=READ STORY,arrowJustify=space-between,arrowJustDiv=span)]"
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        }
      ]
    }
  ]
}